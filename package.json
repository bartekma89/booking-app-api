{
	"name": "server",
	"version": "1.0.0",
	"description": "",
	"type": "module",
	"scripts": {
		"start": "node dist/app.js",
		"build": "tsc",
		"dev": "nodemon -L",
		"typechecking": "tsc --noEmit -p tsconfig.json",
		"lint": "eslint ./src --ext .ts",
		"lint:fix": "eslint ./src --fix",
		"format": "prettier --check ./src",
		"format:fix": "prettier --write ./src",
		"check": "pnpm run lint && pnpm run typechecking",
		"prepare": "husky"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"packageManager": "pnpm@10.19.0",
	"devDependencies": {
		"@eslint/js": "^9.39.1",
		"@types/bcrypt": "^6.0.0",
		"@types/cookie-parser": "^1.4.10",
		"@types/cors": "^2.8.19",
		"@types/express": "^5.0.5",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/morgan": "^1.9.10",
		"@types/node": "^24.10.0",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"globals": "^16.5.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.6",
		"nodemon": "^3.1.10",
		"prettier": "^3.6.2",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.3"
	},
	"imports": {
		"#*": "./src/*"
	},
	"lint-staged": {
		"*.{ts, js}": [
			"eslint",
			"prettier --check"
		],
		"*.{json, md, yml, yaml}": [
			"prettier --check"
		]
	},
	"dependencies": {
		"bcrypt": "^6.0.0",
		"cookie-parser": "^1.4.7",
		"cors": "^2.8.5",
		"dotenv": "^17.2.3",
		"express": "^5.1.0",
		"helmet": "^8.1.0",
		"jsonwebtoken": "^9.0.2",
		"mongoose": "^8.19.3",
		"morgan": "^1.10.1"
	}
}
